    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.4",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^4.0.4"
